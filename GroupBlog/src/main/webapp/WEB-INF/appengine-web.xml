<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>
  <env-variables>
    <env-var name="CLOUD_SQL_CONNECTION_NAME" value="firstgame:us-central1:groupblog" />
    <env-var name="DB_USER" value="root" />
    <env-var name="DB_PASS" value="groupblog" />
    <env-var name="DB_NAME" value="groupblog" />
  </env-variables>
  
  
  
  <static-files>
  <include path="/images/landscape.jpg"></include>
  </static-files>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>

</appengine-web-app>